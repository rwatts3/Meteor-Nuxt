{
  "name": "meteor-nuxt",
  "version": "1.0.0",
  "description": "A wicked experiment combining Nuxt and Meteor",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm:meteor\" \"npm:nuxt\"",
    "meteor": "cd meteor && npm start",
    "nuxt": "cd nuxt && npm run dev",
    "setup": "npm run setup:nuxt && npm run setup:meteor",
    "setup:meteor": "cd meteor && meteor npm i",
    "setup:nuxt": "cd nuxt && npm i"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JorgenVatle/Meteor-Nuxt.git"
  },
  "keywords": [
    "Meteor",
    "Nuxt",
    "Vue",
    "Meteor-Bundle"
  ],
  "author": "JÃ¸rgen Vatle <Jorgen@WcAServices.net> (https://github.com/JorgenVatle)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JorgenVatle/Meteor-Nuxt/issues"
  },
  "homepage": "https://github.com/JorgenVatle/Meteor-Nuxt#readme",
  "devDependencies": {
    "concurrently": "^4.1.0"
  }
}
